{
    "name": "@kion/kg-cli-ui-config-dev-server",
    "version": "2.0.0",
    "description": "Starts an instance of the ui-config-service.",
    "main": "lib/index.js",
    "scripts": {
        "startUiConfigServer": "node bin/start.js",
        "stopUiConfigServer": "node bin/stop.js",
        "jsdoc": "jsdoc ./lib -r -d ./dist/jsdoc/",
        "build-docs": "npm run jsdoc",
        "pub": "npm publish"
    },
    "keywords": [
        "ui-config"
    ],
    "author": "Jasper Vander Velden <jasper.vandervelden@dematic.com>",
    "license": "MIT",
    "files": [
        "bin/",
        "lib/",
        "dist/"
    ],
    "bin": {
        "startUiConfigServer": "./bin/start.js",
        "stopUiConfigServer": "./bin/stop.js"
    },
    "engines": {
        "node": ">=8"
    },
    "dependencies": {
        "dockerode": "^2.5.4",
        "mkdirp": "^0.5.1",
        "rimraf": "^2.6.2"
    },
    "devDependencies": {
        "jsdoc": "^3.5.5"
    }
}
