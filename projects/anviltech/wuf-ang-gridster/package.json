{
    "name": "@anviltech/wuf-ang-gridster",
    "version": "2.0.1",
    "description": "A UI widget for a gridster layout.  Based on [Angular Gridster 2](https://github.com/tiberiuzuld/angular-gridster2).",
    "author": "Darren Luvaas",
    "license": "MIT",
    "scripts": {
        "build": "ng build --prod @anviltech/wuf-ang-gridster",
        "packagr": "yarn build",
        "pub": "yarn packagr && yarn publish dist --access public",
        "prepublish": "yarn packagr"
    },
    "publishConfig": {
        "access": "public"
    },
    "main": "dist/bundles/anviltech-wuf-ang-gridster.umd.js",
    "typings": "dist/anviltech-wuf-ang-gridster.d.ts",
    "dependencies": {
      "angular-gridster2": "~8.2.0"
    },
    "peerDependencies": {
      "@angular/common": "~8.2.14",
    	"@angular/core": "~8.2.14",
      "@angular/material": "~8.2.3",
      "material-design-icons": "^3.0.1"
    }
}
