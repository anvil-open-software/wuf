{
    "name": "@anviltech/wuf-ang-smart-table",
    "version": "2.0.0-rc.3.1",
    "description": "A series of table components (grid) for displaying tabular data",
    "author": "Darren Luvaas",
    "scripts": {
        "build": "ng build --prod @anviltech/wuf-ang-smart-table",
        "packagr": "yarn build",
        "pub": "yarn packagr && yarn publish dist --access public",
        "prepublish": "yarn packagr"
    },
    "publishConfig": {
        "access": "public"
    },
    "main": "dist/bundles/anviltech-wuf-ang-smart-table.umd.js",
    "typings": "dist/anviltech-wuf-ang-smart-table.d.ts",
    "dependencies": {
        "@anviltech/wuf-web-assets": "^2.0.0-rc.1"
    },
    "peerDependencies": {
        "@angular/common": "^7.0.0",
        "@angular/core": "^7.0.0",
        "@angular/material": "7.3.2",
        "@ngx-translate/core": "^11.0.1"
    }
}
