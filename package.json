{
  "name": "@anviltech/wuf-web-ui-framework",
    "version": "2.0.1",
  "author": "Darren Luvaas",
  "repository": {
    "type": "git",
    "url": "https://github.com/anvil-open-software/wuf"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve --aot",
    "start:reset": "yarn build:reset && ng serve --aot",
    "build": "ng build --aot",
    "build:reset": "rm -rf node_modules && yarn bootstrap && yarn build",
    "test": "ng test",
    "test:rebuild": "yarn bootstrap && ng test",
    "test:reset": "rm -rf node_modules && yarn bootstrap && ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "extract-i18n": "ng xi18n --output-path src/locale && xliffmerge --profile xliffmerge.json",
    "bootstrap": "yarn install && lerna bootstrap && lerna run packagr",
    "packagr": "lerna run packagr",
    "lerna:bootstrap": "lerna bootstrap",
    "lerna:build": "lerna run build"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.2.4",
    "@angular/cdk": "~8.2.3",
    "@angular/common": "~8.2.4",
    "@angular/compiler": "~8.2.4",
    "@angular/core": "~8.2.4",
    "@angular/forms": "~8.2.4",
    "@angular/material": "~8.2.3",
    "@angular/platform-browser": "~8.2.4",
    "@angular/platform-browser-dynamic": "~8.2.4",
    "@angular/router": "~8.2.4",
    "@anviltech/wuf-web-assets": "~2.0.0-rc.1",
    "@anviltech/wuf-web-code-sample": "~2.0.0-rc.1",
    "@anviltech/wuf-web-message": "~2.0.0-rc.1",
    "@ngx-translate/core": "^11.0.1",
    "@ngx-translate/http-loader": "^4.0.0",
    "core-js": "~2.5.4",
    "hammerjs": "~2.0.8",
    "material-components-web": "~4.0.0",
    "material-design-icons": "~3.0.1",
    "ngx-md": "~8.1.6",
    "rxjs": "~6.4.0",
    "tslib": "^1.10.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.23",
    "@angular-devkit/build-ng-packagr": "~0.803.23",
    "@angular/cli": "~8.3.2",
    "@angular/compiler-cli": "~8.2.4",
    "@angular/language-service": "~8.2.4",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~5.0.1",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~3.1.0",
    "karma-jasmine-html-reporter": "~1.5.1",
    "karma-mocha-reporter": "~2.2.5",
    "lerna": "~3.4.0",
    "ng-packagr": "~5.4.0",
    "ngx-i18nsupport": "^0.8.6",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tsickle": "~0.37.0",
    "tslint": "~5.15.0",
    "typescript": "~3.5.3"
  },
  "workspaces": [
    "packages/*",
    "projects/anviltech/*"
  ]
}
