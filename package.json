{
  "name": "@anviltech/wuf-web-ui-framework",
  "version": "2.0.0-rc.11",
  "author": "Darren Luvaas",
  "repository": {
    "type": "git",
    "url": "https://github.com/anvil-open-software/wuf"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve --aot",
    "build": "ng build --aot",
    "build-github": "ng build --aot --base-href /wuf/ --deploy-url /wuf/",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "extract-i18n": "ng xi18n --output-path src/locale && xliffmerge --profile xliffmerge.json",
    "bootstrap": "yarn install && lerna bootstrap && lerna run packagr",
    "packagr": "lerna run packagr",
    "lerna:bootstrap": "lerna bootstrap",
    "lerna:build": "lerna run build",
    "unit-test": "ng test --browsers=Chrome",
    "unit-test-circleci": "ng test --browsers=ChromeHeadless --watch=false"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.2.0",
    "@angular/cdk": "7.3.2",
    "@angular/common": "~7.2.0",
    "@angular/compiler": "~7.2.0",
    "@angular/core": "~7.2.0",
    "@angular/forms": "~7.2.0",
    "@angular/http": "~7.2.0",
    "@angular/material": "7.3.2",
    "@angular/platform-browser": "~7.2.0",
    "@angular/platform-browser-dynamic": "~7.2.0",
    "@angular/router": "~7.2.0",
    "@anviltech/wuf-web-assets": "^2.0.0-rc.1",
    "@anviltech/wuf-web-code-sample": "^2.0.0-rc.1",
    "@anviltech/wuf-web-message": "^2.0.0-rc.1",
    "@ngx-translate/core": "^11.0.1",
    "@ngx-translate/http-loader": "^4.0.0",
    "core-js": "^2.5.4",
    "hammerjs": "^2.0.8",
    "material-components-web": "0.44.0",
    "material-design-icons": "3.0.1",
    "ngx-md": "^7.1.3",
    "rxjs": "~6.3.3",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.0",
    "@angular-devkit/build-ng-packagr": "~0.13.0",
    "@angular/cli": "~7.3.1",
    "@angular/compiler-cli": "~7.2.0",
    "@angular/language-service": "~7.2.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.5.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.1.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "lerna": "^3.4.0",
    "ng-packagr": "^4.2.0",
    "ngx-i18nsupport": "^0.8.6",
    "protractor": "~5.4.0",
    "puppeteer": "1.15.0",
    "ts-node": "~7.0.0",
    "tsickle": ">=0.34.0",
    "tslib": "^1.9.0",
    "tslint": "~5.11.0",
    "typescript": "~3.2.2"
  },
  "workspaces": [
    "packages/*",
    "projects/anviltech/*"
  ]
}
